<Window x:Class="Practica0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practica0"
        mc:Ignorable="d"
        Title="Hola mundo" Height="450" Width="800">
    <StackPanel Orientation="Horizontal">
        <Label Name="unaEtiqueta" Content="Primera etiqueta" Margin="40,10" Foreground="Red" FontSize="20" FontWeight="Bold"/>
        <Label Name="dosEtiqueta" Content="Segunda etiqueta" Margin="50" Foreground="Blue"/>
        <StackPanel Orientation="Vertical">
            <Button Name="ButtonFirst" Margin="10" Click="ButtonFirst_Click">
                Primera Etiqueta
            </Button>
            <Button Name="ButtonSecond" Click="ButtonSecond_Click" Content="Segunda Etiqueta" Margin="10"/>
        </StackPanel>
    </StackPanel>
</Window>
